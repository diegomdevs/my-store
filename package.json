{
  "name": "my-store",
  "version": "1.0.0",
  "description": "Learning express",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index",
    "start": "node src/index.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
